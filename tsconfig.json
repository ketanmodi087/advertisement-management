{
	"compilerOptions": {
		"esModuleInterop": true,
		"strict": false,
		"baseUrl": "./app",
		"target": "esnext",
		"module": "commonjs",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"outDir": "./build",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"sourceMap": true,
		"forceConsistentCasingInFileNames": true
	},
	"include": ["app/**/*.ts"],
	"exclude": ["node_modules"],
	"paths": {
		"/*": ["app/*"]
	}
}
